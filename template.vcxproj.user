<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="GodotVersion.props" />
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug_EDITOR_Godot|x64'">
    <LocalDebuggerCommand>$(GodotEditorPath)</LocalDebuggerCommand>
    <LocalDebuggerWorkingDirectory>$(ProjectDir)GodotProject</LocalDebuggerWorkingDirectory>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerCommandArguments>--editor</LocalDebuggerCommandArguments>
    <LocalDebuggerAttach>false</LocalDebuggerAttach>
    <LocalDebuggerDebuggerType>Auto</LocalDebuggerDebuggerType>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug_GAME_Godot|x64'">
    <LocalDebuggerCommand>$(GodotEditorPath)</LocalDebuggerCommand>
    <LocalDebuggerWorkingDirectory>$(ProjectDir)GodotProject</LocalDebuggerWorkingDirectory>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerCommandArguments></LocalDebuggerCommandArguments>
    <LocalDebuggerAttach>false</LocalDebuggerAttach>
    <LocalDebuggerDebuggerType>Auto</LocalDebuggerDebuggerType>
  </PropertyGroup>
</Project>